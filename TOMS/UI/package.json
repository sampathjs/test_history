  "scripts": {
    "postinstall": "husky install && husky add .husky/pre-commit \"npx lint-staged\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup": "npm i && react-scripts build",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint src",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,js,jsx,json}",
    "import-colors": "node scripts/figma/importColors",
    "import-typography": "node scripts/figma/importTypography",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,js,tsx,jsx}": [
      "node ./node_modules/.bin/eslint --cache --fix",
      "node ./node_modules/.bin/prettier --write"
    ],
    "*.{json,md}": [
      "node ./node_modules/.bin/prettier --write"
    ]
  }
}
